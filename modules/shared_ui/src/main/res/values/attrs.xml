<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Other values-->
    <attr name="removeItem" format="reference" />

    <declare-styleable name="TripPlanPlacesIndicatorView">
        <attr name="fitElements" format="boolean"/>
        <attr name="strokeSize" format="float"/>
        <attr name="textBackgroundColor" format="color"/>
        <attr name="itemColor" format="color"/>
        <attr name="fillItems" format="boolean"/>

    </declare-styleable>


    <declare-styleable name="QuadrantLayout_Layout">
        <attr name="layout_viewPosition" format="enum">
            <enum name="topLeft" value="0"/>
            <enum name="topRight" value="1"/>
            <enum name="bottomLeft" value="2"/>
            <enum name="bottomRight" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ToggleLayout">
        <attr name="hideContentByDefault" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="ToggleLayout_Layout">
        <attr name="layout_childDesiredHeight" format="dimension"/>
        <attr name="layout_childDesiredWidth" format="dimension"/>

        <attr name="layout_viewRole" format="enum">
            <enum name="HIDE_BUTTON" value="0"/>
            <enum name="SHOW_BUTTON" value="1"/>
            <enum name="CONTENT" value="2"/>
            <enum name="SECTION_LABEL" value="3"/>
        </attr>
    </declare-styleable>



</resources>